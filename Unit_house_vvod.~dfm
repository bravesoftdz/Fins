  object OD_vvod: TOracleDataSet
    SQL.Strings = (
      
        'select h.kul, h.reu, t.name_reu, s.name, ltrim(h.nd,'#39'0'#39') as nd, ' +
        's.name||'#39' '#39'||ltrim(h.nd,'#39'0'#39') as adr, h.nd as nd_id, r.id, h.id a' +
        's house_id, '
      
        '  r.nm as vvod, r.usl, r.kub, r.vvod_num, r.kub_man, r.kpr, r.ku' +
        'b_sch, r.sch_cnt,'
      
        '  r.sch_kpr, r.cnt_lsk, k.opl, r.rowid, r.vol_add, r.sch_add, r.' +
        'kub_fact, r.vol_add_fact, r.kub_nrm_fact, r.kub_sch_fact,r.itg_f' +
        'act, r.opl_add,'
      '  r.kub_norm, r.use_sch, r.dist_tp, r.rowid'
      
        'from (select c.*, u.nm, u.npp from scott.c_vvod c, scott.usl u w' +
        'here c.usl=u.usl) r, scott.c_houses h, scott.spul s, scott.s_reu' +
        '_trest t,'
      ' (select sum(opl) as opl, house_id from scott.kart k '
      '  where k.psch <> 9 group by k.house_id) k '
      
        'where h.reu=t.reu and h.id=r.house_id(+) and h.id = k.house_id(+' +
        ') and h.kul=s.id(+) and nvl(h.psch,0) <> 1'
      
        'and (:var_=0 or :var_=1 and (nvl(r.kub,0)<>0 or nvl(r.vol_add,0)' +
        '<>0))'
      
        'and (:flt_reu_ is not null and h.reu=:flt_reu_ or :flt_reu_ is  ' +
        'null)'
      'and (:flt_house_ <> -1 and h.id=:flt_house_ or :flt_house_ = -1)'
      
        'and (:flt_kul_ is not null and h.kul=:flt_kul_ or :flt_kul_ is  ' +
        'null)'
      
        'order by s.name, scott.utils.f_order(h.nd, 6), r.npp, nvl(r.vvod' +
        '_num,0)')
    Optimize = False
    Variables.Data = {
      0300000004000000050000003A5641525F030000000000000000000000090000
