  object OD_rep: TOracleDataSet
    SQL.Strings = (
      'select h.kul, h.reu, t.name_reu, s.name, ltrim(h.nd,'#39'0'#39') as nd, '
      
        's.name||'#39' '#39'||ltrim(h.nd,'#39'0'#39') as adr, h.nd as nd_id, v.id, h.id a' +
        's house_id, '
      
        '  v.nm as vvod, v.kub, v.vvod_num, v.kub_man, v.kub_norm, v.kpr,' +
        ' v.kub_sch, v.sch_cnt,'
      
        '  v.sch_kpr, v.cnt_lsk, k.opl, v.rowid, v.vol_add, v.sch_add, v.' +
        'kub_fact, v.vol_add_fact, v.itg_fact, v.opl_add,'
      
        '  scott.utils.month_name(substr(:mg, 5, 2))||'#39' '#39'||substr(:mg,1,4' +
        ')||'#39#1075'.'#39' as mg'
      
        'from (select c.*, u.nm from scott.c_vvod c, scott.usl u where c.' +
        'usl=u.usl'
      
        'and (:var_=0 or :var_=1 and (nvl(c.kub,0)<>0 or nvl(c.vol_add,0)' +
        '<>0))'
      ') v, scott.c_houses h, scott.spul s, scott.s_reu_trest t,'
      ' (select sum(opl) as opl, house_id from scott.kart k '
      '  where k.psch <> 9 group by k.house_id) k'
      
        'where h.reu=t.reu and h.id=v.house_id and h.id = k.house_id(+) a' +
        'nd h.kul=s.id(+) and nvl(h.psch,0) <> 1'
      ''
      
        'and (:flt_reu_ is not null and h.reu=:flt_reu_ or :flt_reu_ is  ' +
        'null)'
      'and (:flt_house_ <> -1 and h.id=:flt_house_ or :flt_house_ = -1)'
      
        'and (:flt_kul_ is not null and t.reu=:flt_kul_ or :flt_kul_ is  ' +
        'null)'
      
        'order by t.name_reu, s.name||'#39' '#39'||ltrim(h.nd,'#39'0'#39'), v.nm, v.vvod_' +
        'num'
      '')
    Optimize = False
    Variables.Data = {
      0300000005000000030000003A4D47050000000000000000000000050000003A
      5641525F030000000000000000000000090000003A464C545F5245555F050000
      0000000000000000000B0000003A464C545F484F5553455F0300000000000000
      00000000090000003A464C545F4B554C5F050000000000000000000000}
    QBEDefinition.QBEFieldDefs = {
      040000001B00000003000000524555010000000000040000004E414D45010000
      000000020000004E440100000000000400000056564F44010000000000030000
