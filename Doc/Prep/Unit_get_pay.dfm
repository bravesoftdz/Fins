  object OD_oper: TOracleDataSet
    SQL.Strings = (
      'select o.oper, o.oper||'#39' '#39'||o.naim  as naim '
      ' from scott.oper o'
      'where o.fk_org=scott.init.get_org_nkom'
      'union all'
