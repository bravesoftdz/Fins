  object OD_c_kwtp: TOracleDataSet
    SQL.Strings = (
      'select t.lsk,  '
      
        's.name||'#39', '#39'||NVL(LTRIM(k.nd,'#39'0'#39'),'#39'0'#39')||'#39'-'#39'||NVL(LTRIM(k.kw,'#39'0'#39')' +
        ','#39'0'#39')'
      'as adr, t.summa, t.penya, t.oper, '
      't.nink, t.nkom, t.dtek, t.nkvit, t.dat_ink, '
      
        'substr(t.dopl,1,4)||'#39'-'#39'||substr(t.dopl,5,2) as dopl, t.ts, t.id,' +
        ' t.iscorrect '
      'from scott.c_kwtp t, scott.kart k, scott.spul s'
      'where oper = :oper and k.lsk=t.lsk and k.kul=s.id'
      'and t.nkom=scott.init.get_nkom()'
      'and  ((:var =1) or (:var =0 and  t.nink = 0))'
      'order by t.ts')
    Optimize = False
