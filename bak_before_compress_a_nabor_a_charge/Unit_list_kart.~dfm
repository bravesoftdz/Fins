  object OD_list_kart: TOracleDataSet
    SQL.Strings = (
      'select /*+ FIRST_ROWS */ k.rowid, '
      
        'r.name_reu, k.reu, k.k_lsk_id, k.c_lsk_id, k.lsk, k.kul, s.name,' +
        ' ltrim(k.nd,'#39'0'#39') as n_nd, ltrim(k.kw,'#39'0'#39') as n_kw,'
      'k.nd,k.kw, k.kfg,'
      
        'k.fio, k.k_im, k.k_ot, k.k_fam,k.psch, k.mgw,k.mhw, k.mel, k.pgw' +
        ', k.phw, k.pel,'
      
        'k.et, k.kpr, k.kpr_ot, k.kpr_wr, k.ki, k.opl, k.ppl, k.komn, k.s' +
        'tatus, k.fk_deb_org,'
      't.name as n_status, k.fk_distr,'
      
        'k.eksub1, k.eksub2, k.sgku, k.doppl, k.subs_cor, k.subs_cur, k.k' +
        'ran1,'
      'k.el, k.el1, case when k.subs_cur = 1 or  nvl(k.subs_cor,0) <>0 '
      'then 1 else 0 end as subs_val, '
      
        'scott.init.get_nkom as nkom, k.sub_data, decode(k.psch, 0, '#39#1053#1086#1088#1084 +
        #39','
      
        '1,'#39#1061'.'#1042'. '#1080' '#1043'.'#1042'.'#39', 2, '#39#1061'.'#1042'.'#39', 3, '#39#1043'.'#1042'.'#39') as psch_name, k.sch_el, k' +
        '.house_id,'
      'k.kan_sch, k.dog_num, k.schel_dt, k.psch_dt, '
      'k.text,k.memo,k.schel_end,  k.polis, k.law_doc, k.law_doc_dt, '
      'k.prvt_doc, k.prvt_doc_dt, k.fk_pasp_org, k.cpn,'
      
        'decode(mg1, '#39'000000'#39', '#39'...'#39', substr(mg1,1,4)||'#39'-'#39'||substr(mg1,5,' +
        '2))  as mg1,'
      
        'decode(mg2, '#39'999999'#39', '#39'...'#39', substr(mg2,1,4)||'#39'-'#39'||substr(mg2,5,' +
        '2))  as mg2'
      
        'from scott.kart k, scott.spul s, scott.status t, scott.s_reu_tre' +
        'st r'
      'where k.kul=s.id and k.status=t.id and k.reu=r.reu'
      ''
      
        'and (:flt_reu_ is not null and k.reu=:flt_reu_ or :flt_reu_ is  ' +
        'null)'
      
        'and (:flt_kul_ is not null and k.kul=:flt_kul_ or :flt_kul_ is  ' +
        'null)'
      
        'and (:flt_nd_ is not null and k.nd=:flt_nd_ or :flt_nd_ is  null' +
        ')'
      
        'and (:flt_kw_ is not null and k.kw=:flt_kw_ or :flt_kw_ is  null' +
        ')'
      
        'and (:flt_k_lsk_id_ <> -1 and k.k_lsk_id=:flt_k_lsk_id_ or :flt_' +
        'k_lsk_id_ = -1)'
      ''
      'and (:var1_=0 or (:var1_=1 and k.psch<>8)) and (:var_=0'
      'or (:var_=5 and k.dog_num like upper(:str1_)||'#39'%'#39')'
      ''
      '/*or (:var_=4 and k.k_lsk_id = :k_lsk_id_)*/'
      ''
      '/*or (:var_=3 and k.kul = :kul_ and k.nd = :nd_  and k.kw= :kw_)'
      'or (:var_=2 and k.kul = :kul_ and k.nd = :nd_)'
      'or (:var_=1 and k.kul = :kul_)*/'
      ''
      'or (:var_=6 and'
      ' exists '
      
        '  (select * from scott.kart k1 where k1.lsk=k.lsk and upper(k1.f' +
        'io) like '#39'%'#39'||upper(:str1_)||'#39'%'#39')'
      '  ) '
      'or (:var_=7 and'
      ' exists '
      
        '  (select * from scott.c_kart_pr k1 where k1.lsk=k.lsk and upper' +
        '(k1.fio) like '#39'%'#39'||upper(:str1_)||'#39'%'#39')'
      '  )'
      ')'
      'order by s.name, k.nd, k.kw, decode(k.psch,8,1,9,1,0)')
