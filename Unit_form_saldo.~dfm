  object OD_saldo: TOracleDataSet
    SQL.Strings = (
      'select t.lsk, u.usl, u.nm, s.kod, s.name, t.summa  '
      'from scott.saldo_usl t, scott.usl u,'
      'scott.sprorg s'
      'where t.usl=u.usl and'
      't.lsk=:lsk and t.org=s.kod and t.mg=:mg'
      'union all'
      'select t.lsk, null, '#39#1048#1090#1086#1075#1086':'#39', null, null, sum(t.summa )'
      'from scott.saldo_usl t'
      'where t.lsk=:lsk and t.mg=:mg'
      'group by t.lsk'
      'order by lsk, usl, kod')
    Optimize = False
    Variables.Data = {
