        'select p.naim as oper_name, t.summa,t.penya,substr(t.dopl,5,2)||' +
        #39'.'#39'||substr(t.dopl,1,4) as dopl'
      ' from scott.a_kwtp_mg t,scott.oper p'
      ' where t.c_kwtp_id=:pay_id'
