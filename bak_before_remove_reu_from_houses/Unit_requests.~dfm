  object OD_request: TOracleDataSet
    SQL.Strings = (
      'select t.rowid, t.id,'
      '       t.name,'
      '       r.name  as job,'
      '       o1.name as user_fio,'
      '       t.dt1,'
      '       t.dt2,'
      '       d.f_s1  as r_fio,'
      '       d.f_c1  as memo2,'
      '       t.fk_doctp,'
      '       t.fk_klsk'
      '  from scott.t_doc t,'
      '       scott.t_doc_tp tp,'
      '       scott.v_cu_rlxac v,'
      '       scott.t_doc_det d,'
      '       scott.t_doc_det d1,'
      '       scott.t_role r,'
      '       (select max(x.id) as id, x.fk_doc'
      '          from scott.t_docxa x, scott.t_act a'
      '         where x.fk_act = a.id'
      '           and a.cd = '#39#1057#1086#1079#1076#1072#1090#1100#39
      '         group by x.fk_doc) o,'
      
        '       (select x.id, u.name from scott.t_docxa x, scott.t_user u' +
        ' where x.fk_user = u.id) o1'
      ' where t.fk_doctp = tp.id'
      '   and v.fk_doctp = tp.id'
      '   and tp.cd = '#39#1047#1072#1103#1074#1082#1072#39
      '   and t.id = d.fk_doc(+)'
      '   and t.id = d1.fk_doc(+)'
      '   and d.npp(+) = 1'
      '   and d1.npp(+) = 2'
      '   and t.id = o.fk_doc'
      '   and o.id = o1.id'
      '   and v.fk_role = r.id')
    Optimize = False
    SequenceField.Field = 'ID'
    SequenceField.Sequence = 'scott.t_doc_id'
    SequenceField.ApplyMoment = amOnNewRecord
    QBEDefinition.QBEFieldDefs = {
      040000000A000000030000004A4F4201010100000005000000525F46494F0100
      00000000050000004D454D4F3201010000000008000000555345525F46494F01
      010000000002000000494401000000000007000000464B5F4B4C534B01000000
      0000030000004454310100000000000300000044543201000000000004000000
      4E414D4501000000000008000000464B5F444F435450010000000000}
    RefreshOptions = [roBeforeEdit, roAfterInsert, roAfterUpdate, roAllFields]
    BeforeQBE = OD_requestBeforeQBE
    AfterQBE = OD_requestAfterQBE
    UpdatingTable = 'scott.t_doc'
    CommitOnPost = False
    CachedUpdates = True
    Session = DataModule1.OracleSession1
    BeforeScroll = OD_requestBeforeScroll
    AfterScroll = OD_requestAfterScroll
    Left = 8
    Top = 80
    object OD_requestNAME: TStringField
      DisplayLabel = #8470
      FieldName = 'NAME'
      Visible = False
      Size = 32
    end
    object OD_requestDT1: TDateTimeField
      DisplayLabel = #1044#1072#1090#1072
      DisplayWidth = 9
      FieldName = 'DT1'
    end
