  object OD_mg: TOracleDataSet
    SQL.Strings = (
      'select mg, substr(mg,1,4)||'#39'-'#39'||substr(mg,5,2) as mg1 from '
      'scott.period_reports p where p.id=51'
      'union all'
      'select p.period as mg, '#39#1090#1077#1082#1091#1097#1080#1081#39' as mg1 from scott.params p'
      'union all'
      'select '#39'999999'#39' as mg, '#39#1074#1089#1077' '#1087#1077#1088#1080#1086#1076#1099#39' as mg1 from scott.params p'
      'order by mg desc')
    Optimize = False
    QBEDefinition.QBEFieldDefs = {
      0400000002000000020000004D47010000000000030000004D47310100000000
      00}
    Session = DataModule1.OracleSession1
    Active = True
    Left = 72
    Top = 80
    object OD_mgMG: TStringField
      FieldName = 'MG'
      Size = 6
    end
    object OD_mgMG1: TStringField
      DisplayWidth = 15
      FieldName = 'MG1'
      Size = 15
    end
  end
