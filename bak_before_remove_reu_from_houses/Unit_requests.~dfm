  object OD_adr: TOracleDataSet
    SQL.Strings = (
      
        'select s.name||'#39', '#39'||ltrim(t.nd,'#39'0'#39')||'#39'-'#39'||ltrim(t.kw,'#39'0'#39') as ad' +
        'r'
      ' from scott.kart t, scott.spul s where t.kul=s.id'
      'and t.k_lsk_id=:k_lsk_id')
    Optimize = False
    Variables.Data = {
      0300000001000000090000003A4B5F4C534B5F494403000000040000006B6201
      0000000000}
