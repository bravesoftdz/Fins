  object OD_user_perm: TOracleDataSet
    SQL.Strings = (
      
        'select c.*, c.rowid, t.reu||'#39'-'#39'||t.name_reu as name_reu, o.name ' +
        'as name_org'
      'from scott.c_users_perm c, scott.s_reu_trest t, scott.t_org o'
      'where c.user_id = :user_id and c.fk_perm_tp=:permtp'
      'and c.fk_reu=t.reu(+) and c.fk_pasp_org=o.id(+)'
      'order by t.name_reu, o.name')
    Optimize = False
    Variables.Data = {
