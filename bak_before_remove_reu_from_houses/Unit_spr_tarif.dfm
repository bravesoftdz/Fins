  object OD_spr_tarif: TOracleDataSet
    SQL.Strings = (
      
        'select t.name, '#39#39' as tp_name, 0 as tp, t.id, null as parent_id, ' +
        'null as cena, t.cd, t.cdtp from scott.spr_tarif t'
      '  where t.cd='#39'000'#39' '
      'union all'
      
        'select t.name, '#39#1055#1072#1082#1077#1090#39' as tp_name, 1 as tp, t.id, p.fk_tarif as ' +
        'parent_id, a.cena, t.cd, t.cdtp from scott.spr_tarif t, scott.us' +
        'l u,'
      '  (select s.* from scott.spr_tarif_prices s, scott.params p'
      '  where p.period between s.mg1 and s.mg2) a,'
      '  scott.spr_tarifxprogs p '
      '  where t.id=p.fk_prog and t.usl=:usl and t.usl=u.usl and'
      '   p.fk_prog=a.fk_tarif(+)'
      
        '  and not exists (select * from scott.spr_tarif x, scott.spr_tar' +
        'ifxprogs k where k.fk_prog=t.id '
      '  and k.fk_tarif=x.id and x.cd<>'#39'000'#39')'
      'union all'
      
        'select t.name, '#39#1055#1088#1086#1075#1088#1072#1084#1084#1072#39' as tp_name, 2 as tp, t.id, p.fk_tarif' +
        ' as parent_id, a.cena, t.cd, t.cdtp from scott.spr_tarif t, scot' +
        't.usl u,'
      '  (select s.* from scott.spr_tarif_prices s, scott.params p'
      '  where p.period between s.mg1 and s.mg2) a,'
      '  scott.spr_tarifxprogs p '
      '  where t.id=p.fk_prog and t.usl=:usl and t.usl=u.usl and'
      '   p.fk_prog=a.fk_tarif(+)'
      
        '  and exists (select * from scott.spr_tarif x, scott.spr_tarifxp' +
        'rogs k where k.fk_prog=t.id '
      '  and k.fk_tarif=x.id and x.cd<>'#39'000'#39')')
    Optimize = False
