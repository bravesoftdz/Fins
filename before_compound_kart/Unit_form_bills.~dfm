  object OD_kart: TOracleDataSet
    SQL.Strings = (
      
        'select k.lsk, s.name as street, nvl(ltrim(k.nd, '#39'0'#39'), '#39'0'#39') as nd' +
        ','
      
        '       nvl(ltrim(k.kw, '#39'0'#39'), '#39'0'#39') as kw, k.opl, k.fio, k.kpr, a.' +
        'summa, b.penya'
      '  from scott.kart k, scott.spul s, '
      
        ' (select sum(summa) as summa from scott.a_kwtp_mg t, scott.kart ' +
        'k where k.lsk=t.lsk '
      
        '  and k.k_lsk_id=scott.utils.get_k_lsk_id_by_lsk(:lsk) and t.mg ' +
        'between :mg1_ and :mg2_'
      '  and (:var_=0 and k.psch<>8 or :var_=1) ) a,'
      
        ' (select sum(t.penya) as penya from scott.arch_kart t, scott.kar' +
        't k where k.lsk=t.lsk and  '
      
        '  k.k_lsk_id=scott.utils.get_k_lsk_id_by_lsk(:lsk) and t.mg betw' +
        'een :mg1_ and :mg2_'
      '  and (:var_=0 and k.psch<>8 or :var_=1) ) b'
      ' where k.kul = s.id'
      ' and k.lsk=:lsk')
    Optimize = False
    Variables.Data = {
      0300000004000000040000003A4C534B05000000000000000000000005000000
      3A4D47315F050000000000000000000000050000003A4D47325F050000000000
      000000000000050000003A5641525F030000000000000000000000}
